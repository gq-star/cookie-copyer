{
  "name": "Cookie Copyer",
  "description": "an Extension for copy all cookies under a specific domain to current page!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "cookies"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/cookie16.png",
      "32": "/images/cookie32.png",
      "48": "/images/cookie48.png",
      "128": "/images/cookie128.png"
    }
  },
  "icons": {
    "16": "/images/cookie16.png",
    "32": "/images/cookie32.png",
    "48": "/images/cookie48.png",
    "128": "/images/cookie128.png"
  }
}
